<Window x:Class="Presentacion.WindowPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentacion"
        mc:Ignorable="d"
        Title="WindowPrincipal" Height="320" Width="615" Background="#FFF9D9C3">
    <Grid Margin="0,0,10,-6">
        <Button x:Name="btnVerConcesionarios" Content="Ver Concesionario"
                HorizontalAlignment="Center" Margin="0,152,0,0"
                VerticalAlignment="Top" Height="32" Width="128"
                FontSize="14" Click="btnVerConcesionarios_Click"/>

        <Button x:Name="btnSalir" Content="Salir"
                HorizontalAlignment="Center" Margin="0,207,0,0"
                VerticalAlignment="Top" Height="32" Width="128"
                RenderTransformOrigin="0.058,-0.565" FontSize="14"
                Click="btnSalir_Click"/>
        <Label Content="Menú Principal" HorizontalAlignment="Center" Margin="0,56,0,0" VerticalAlignment="Top" FontSize="36"/>
    </Grid>
</Window>
